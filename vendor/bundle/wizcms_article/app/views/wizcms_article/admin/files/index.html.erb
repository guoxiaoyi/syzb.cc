<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @files.each do |file| %>
      <tr>
        <td><%= image_tag file.asset.url, width: '200' %></td>
        <td><%= link_to '删除', admin_file_path(file), method: :delete, data: { confirm: '你确定吗?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @files, theme: 'bootstrap3' %>
