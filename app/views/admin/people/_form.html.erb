<%= simple_form_for([:admin, @person], wrapper: :bootstrap3) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :nianji %>
    <%= f.input :city %>
    <%= f.input :email %>
    <%= f.input :leixing %>
    <%= f.input :content %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
